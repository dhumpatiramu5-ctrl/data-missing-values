 NaN           8\n"]}]},{"cell_type":"code","source":["# Least Squares Method - Manual Implementation\n","\n","# Sample data\n","x = [1, 2, 3, 4, 5]\n","y = [2, 4, 5, 4, 5]\n","\n","n = len(x)\n","\n","# Calculate required sums\n","sum_x = sum(x)\n","sum_y = sum(y)\n","sum_x2 = sum(xi**2 for xi in x)\n","sum_xy = sum(x[i] * y[i] for i in range(n))\n","\n","# Least squares formulas\n","m = (n * sum_xy - sum_x * sum_y) / (n * sum_x2 - sum_x**2)\n","c = (sum_y - m * sum_x) / n\n","\n","# Output\n","print(\"Slope (m):\", m)\n","print(\"Intercept (c):\", c)\n","\n","# Predicted values\n","print(\"\\nPredicted values:\")\n","for xi in x:\n","    yi = m * xi + c\n","    print(f\"x = {xi}, y = {yi:.2f}\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vltojfAvQAS2","executionInfo":{"status":"ok","timestamp":1767333202513,"user_tz":-330,"elapsed":42,"user":{"displayName":"dhumpati ramu","userId":"14364464204857685832"}},"outputId":"15b03545-19ff-4ed7-e3a3-3655d8f52b2c"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Slope (m): 0.6\n","Intercept (c): 2.2\n","\n","Predicted values:\n","x = 1, y = 2.80\n","x = 2, y = 3.40\n","x = 3, y = 4.00\n","x = 4, y = 4.60\n","x = 5, y = 5.20\n"]}]},{"cell_type":"code","source":["import numpy as np\n","import pandas as pd\n","import random\n","from scipy import stats\n","\n","# Step 1: Define treatments and experimental units\n","treatments = ['T1', 'T2', 'T3']\n","units_per_treatment = 2\n","total_units = units_per_treatment * len(treatments)\n","\n","# Step 2: Random assignment (CRD)\n","treatment_list = treatments * units_per_treatment\n","random.shuffle(treatment_list)\n","\n","# Step 3: Generate response values (example data)\n","np.random.seed(10)\n","response = np.random.normal(15, 5, total_units)\n","\n","# Step 4: Create DataFrame\n","df = pd.DataFrame({\n","    'Unit': range(1, total_units + 1),\n","    'Treatment': treatment_list,\n","    'Response': response\n","})\n","\n","print(\"Completely Randomized Design Data:\")\n","print(df)\n","\n","# Step 5: Perform One-Way ANOVA (CRD analysis)\n","grouped_data = [\n","    df[df['Treatment'] == t]['Response']\n","    for t in treatments\n","]\n","\n","f_stat, p_value = stats.f_oneway(*grouped_data)\n","print(\"\\nANOVA Results:\")\n","print(\"F-statistic:\", round(f_stat, 4))\n","print(\"P-value:\", round(p_value, 4))\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"DTNGTCJrVpIK","executionInfo":{"status":"ok","timestamp":1767334541284,"user_tz":-330,"elapsed":2702,"user":{"displayName":"dhumpati ramu","userId":"14364464204857685832"}},"outputId":"dea5bb24-fe88-45e9-b2c9-9a41215b1ecf"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Completely Randomized Design Data:\n","   Unit Treatment   Response\n","0     1        T1  21.657933\n","1     2        T2  18.576395\n","2     3        T3   7.272999\n","3     4        T2  14.958081\n","4     5        T3  18.106680\n","5     6        T1  11.399572\n","\n","ANOVA Results:\n","F-statistic: 0.2666\n","P-value: 0.7824\n"]}]},{"cell_type":"code","source":["import pandas as pd\n","import numpy as np\n","\n","# Step 1: Create a sample dataset\n","data = {\n","    'Age': [18, 20, np.nan, 22, 21, np.nan, 19],\n","    'Score': [85, np.nan, 78, 90, np.nan, 88, 76]\n","}\n","\n","df = pd.DataFrame(data)\n","\n","print(\"Original Dataset:\")\n","print(df)\n","\n","# Step 2: Median Imputation\n","for column in df.columns:\n","    median_value = df[column].median()\n","    df[column].fillna(median_value, inplace=True)\n","\n","# Step 3: Display processed data\n","print(\"\\nDataset After Median Imputation:\")\n","print(df)\n"],"metadata":{"id":"XW2aS18VX4Ug","executionInfo":{"status":"ok","timestamp":1767335108755,"user_tz":-330,"elapsed":20,"user":{"displayName":"dhumpati ramu","userId":"14364464204857685832"}},"outputId":"57a4fae9-4768-479f-de4e-ac72df22b54a","colab":{"base_uri":"https://localhos